{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "target": "ES2018",
    "module": "CommonJS",
    "moduleDetection": "force",
    "jsx": "react-jsx",
    "allowJs": true,
    "noEmit": false, // Altere para false ou remova esta linha
    "outDir": "dist", // Adicione essa linha para especificar a pasta de sa√≠da
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noPropertyAccessFromIndexSignature": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}